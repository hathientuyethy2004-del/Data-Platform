# Run Day 14 Gate 2 re-check every hour on minute 15 (UTC)
# Install: crontab infrastructure/docker/migration-day4/scripts/day14_gate2_hourly.cron.example

SHELL=/bin/bash
PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin

15 * * * * cd /workspaces/Data-Platform && bash infrastructure/docker/migration-day4/scripts/run_day14_gate2_hourly.sh >> documentation/migration/reports/DAY14_GATE2_CRON.log 2>&1
