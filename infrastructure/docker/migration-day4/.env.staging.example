# Airflow bootstrap
AIRFLOW_DB_NAME=airflow
AIRFLOW_DB_USER=airflow
AIRFLOW_DB_PASSWORD=replace-with-strong-password
AIRFLOW_ADMIN_USER=admin
AIRFLOW_ADMIN_PASSWORD=replace-with-strong-password

# Storage connection (OSS target)
STORAGE_BACKEND=s3
S3_ENDPOINT_URL=http://minio:9000
S3_BUCKET=data-platform-lakehouse
S3_ACCESS_KEY_ID=replace-with-access-key
S3_SECRET_ACCESS_KEY=replace-with-secret-key

# Secret manager integration (example)
SECRET_PROVIDER=vault
VAULT_ADDR=http://vault:8200
VAULT_TOKEN=replace-with-vault-token
VAULT_KV_PATH=secret/data-platform/staging

# Migration flags (Day 3 output)
USE_OSS_INGESTION=false
USE_OSS_QUALITY=false
USE_OSS_SERVING=false
READ_FROM_OSS_SERVING=false
DUAL_RUN_ENABLED=true
DUAL_RUN_COMPARE_ENABLED=true
